    <sql>update staging_fluxo_recebiveis A&#xd;&#xa;inner join &#x28;select max&#x28;parcela&#x29; parcela, cpf, data_contrato from staging_fluxo_recebiveis&#xd;&#xa;group by cpf, data_contrato&#x29; B&#xd;&#xa;on A.cpf &#x3d; B.cpf and A.data_contrato &#x3d; B.data_contrato&#xd;&#xa;set A.ultimaparcela &#x3d; B.parcela&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;update staging_fluxo_recebiveis&#xd;&#xa;set status &#x3d; &#x27;aberto&#x27;&#xd;&#xa;where  now&#x28;&#x29; &#x3c;&#x3d; vencimento_parcela &#x3b;&#xd;&#xa;&#xd;&#xa;update staging_fluxo_recebiveis&#xd;&#xa;set status &#x3d; &#x27;atrasado&#x27;&#xd;&#xa;where  now&#x28;&#x29; &#x3e; vencimento_parcela &#xd;&#xa;and pagamento &#x3d; &#x27;false&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;update staging_fluxo_recebiveis&#xd;&#xa;set status &#x3d; &#x27;quitado&#x27;&#xd;&#xa;where pagamento &#x3d; &#x27;true&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;-- descobre se &#xe9; renovacao&#xd;&#xa;update staging_fluxo_recebiveis A &#xd;&#xa;inner join &#x28;select count&#x28;distinct data_contrato&#x29; qtde ,cpf from staging_fluxo_recebiveis group by&#xd;&#xa;cpf having count&#x28;distinct data_contrato&#x29; &#x3c;2&#x29; B&#xd;&#xa;on A.cpf &#x3d; B.cpf&#xd;&#xa;set renovacao &#x3d; 0&#x3b;&#xd;&#xa;&#xd;&#xa;-- para os que tem mais de um contrato, o primerio quitado na&#xf5; &#xe9; renovacao&#xd;&#xa;update staging_fluxo_recebiveis Z inner join&#xd;&#xa;&#x28;&#xd;&#xa;select distinct cpf, data_contrato from staging_fluxo_recebiveis where cpf in &#x28;&#xd;&#xa;select cpf from staging_fluxo_recebiveis group by&#xd;&#xa;cpf having count&#x28;distinct data_contrato&#x29; &#x3e;1&#xd;&#xa;&#x29;&#xd;&#xa;and status&#x3d;&#x27;quitado&#x27;&#xd;&#xa;group by cpf, data_contrato, status&#xd;&#xa;having count&#x28;distinct status&#x29; &#x3c;2&#xd;&#xa;&#x29; X&#xd;&#xa;on X.cpf &#x3d; Z.cpf and X.data_contrato &#x3d; Z.data_contrato&#xd;&#xa;set renovacao &#x3d; 0&#x3b;&#xd;&#xa;&#xd;&#xa;-- para os que tem mais de um contrato, os outros depois do quitado &#xd;&#xa;&#xd;&#xa;update staging_fluxo_recebiveis Z inner join&#xd;&#xa;&#x28;&#xd;&#xa;select distinct cpf, data_contrato from staging_fluxo_recebiveis where cpf in &#x28;&#xd;&#xa;select cpf from staging_fluxo_recebiveis group by&#xd;&#xa;cpf having count&#x28;distinct data_contrato&#x29; &#x3e;1&#xd;&#xa;&#x29;&#xd;&#xa;and status&#x3d;&#x27;quitado&#x27;&#xd;&#xa;group by cpf, data_contrato, status&#xd;&#xa;having count&#x28;distinct status&#x29; &#x3c;2&#xd;&#xa;&#x29; X&#xd;&#xa;on X.cpf &#x3d; Z.cpf and X.data_contrato not in &#x28; Z.data_contrato&#x29;&#xd;&#xa;set renovacao &#x3d; 1&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- inicia regra de neg&#xf3;cio&#xd;&#xa;-- coloca comissao na primeira parcela&#xd;&#xa;update staging_fluxo_recebiveis A &#xd;&#xa;inner join regra_comissao B on &#x28; A.produto &#x3d; B.product and A.parceiro &#x3d; B.parceiro and A.renovacao &#x3d; B.renovacao &#x29;&#xd;&#xa;set valor_a_receber &#x3d; round&#x28;VGV &#x2a; comissao,2&#x29;&#xd;&#xa;where A.parcela &#x3d; 1 and B.parcela &#x3d; 1&#xd;&#xa;and pagamento &#x3d; &#x27;true&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;-- quem paga na cabe&#xe7;a&#xd;&#xa;update staging_fluxo_recebiveis A &#xd;&#xa;inner join regra_comissao B on &#x28; A.produto &#x3d; B.product and A.parceiro &#x3d; B.parceiro and A.renovacao &#x3d; B.renovacao &#x29;&#xd;&#xa;set valor_a_receber &#x3d; round&#x28;VGV &#x2a; comissao,2&#x29;&#xd;&#xa;where A.parcela &#x3d; 1&#xd;&#xa;and B.parcela &#x3d;0&#x3b;&#xd;&#xa;-- and pagamento &#x3d; &#x27;true&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;-- quem recebe na ultima&#xd;&#xa;update staging_fluxo_recebiveis A &#xd;&#xa;inner join regra_comissao B on &#x28; A.produto &#x3d; B.product and A.parceiro &#x3d; B.parceiro and A.renovacao &#x3d; B.renovacao &#x29;&#xd;&#xa;set valor_a_receber &#x3d; round&#x28;VGV &#x2a; comissao,2&#x29;&#xd;&#xa;where A.parcela &#x3d; A.ultimaparcela&#xd;&#xa;and B.parcela &#x3d;-1&#xd;&#xa;and status in &#x28;&#x27;aberto&#x27;,&#x27;quitado&#x27;&#x29;&#x3b;&#xd;&#xa;-- and pagamento &#x3d; &#x27;true&#x27;&#x3b;&#xd;&#xa;&#xd;&#xa;-- quem recebe na N parcela&#xd;&#xa;update staging_fluxo_recebiveis A &#xd;&#xa;inner join regra_comissao B on &#x28; A.produto &#x3d; B.product and A.parceiro &#x3d; B.parceiro and A.renovacao &#x3d; B.renovacao &#x29;&#xd;&#xa;set valor_a_receber &#x3d; round&#x28;VGV &#x2a; &#x28;comissao&#x2f;ultimaparcela&#x29;,2&#x29;&#xd;&#xa;where A.parcela not in &#x28;1&#x29;&#xd;&#xa;and B.parcela &#x3d;-9&#xd;&#xa;and status in &#x28;&#x27;aberto&#x27;,&#x27;quitado&#x27;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- zera a partir dos caras que est&#xe3;o em atraso&#xd;&#xa;update staging_fluxo_recebiveis A inner join&#xd;&#xa;&#x28;select cpf,parcela,data_contrato from staging_fluxo_recebiveis where status &#x3d;&#x27;atrasado&#x27;&#x29; B&#xd;&#xa;on A.cpf &#x3d; B.cpf and B.data_contrato &#x3d; B.data_contrato and  A.parcela &#x3e;&#x3d; B.parcela&#xd;&#xa;set valor_a_receber &#x3d; 0&#x3b;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;-- data recebimento&#xd;&#xa;update staging_fluxo_recebiveis&#xd;&#xa;set data_recebimento &#x3d; concat&#x28;year&#x28;vencimento_parcela&#x29;,&#x27;-&#x27;,right&#x28;concat&#x28;&#x27;00&#x27;,month&#x28;vencimento_parcela&#x29;&#x29;,2&#x29;,&#x27;-05&#x27;&#x29;&#xd;&#xa;where day&#x28;vencimento_parcela&#x29; between 1 and 4&#x3b;&#xd;&#xa;&#xd;&#xa;update staging_fluxo_recebiveis&#xd;&#xa;set data_recebimento &#x3d; concat&#x28;year&#x28;vencimento_parcela&#x29;,&#x27;-&#x27;,right&#x28;concat&#x28;&#x27;00&#x27;,month&#x28;vencimento_parcela&#x29;&#x29;,2&#x29;,&#x27;-20&#x27;&#x29;&#xd;&#xa;where day&#x28;vencimento_parcela&#x29; between 5 and 19&#x3b;&#xd;&#xa;&#xd;&#xa;update staging_fluxo_recebiveis&#xd;&#xa;set data_recebimento &#x3d; concat&#x28;year&#x28;vencimento_parcela&#x29;,&#x27;-&#x27;,right&#x28;concat&#x28;&#x27;00&#x27;,month&#x28;vencimento_parcela&#x29;&#x2b;1&#x29;,2&#x29;,&#x27;-05&#x27;&#x29;&#xd;&#xa;where day&#x28;vencimento_parcela&#x29; between 20 and 31&#xd;&#xa;and month&#x28;vencimento_parcela&#x29; &#x3c;&#x3e; 12&#x3b;&#xd;&#xa;&#xd;&#xa;update staging_fluxo_recebiveis&#xd;&#xa;set data_recebimento &#x3d; concat&#x28;year&#x28;vencimento_parcela&#x29;&#x2b;1,&#x27;-01-05&#x27;&#x29;&#xd;&#xa;where day&#x28;vencimento_parcela&#x29; between 20 and 31&#xd;&#xa;and month&#x28;vencimento_parcela&#x29; &#x3d; 12&#x3b;&#xd;&#xa;&#xd;&#xa;</sql>
