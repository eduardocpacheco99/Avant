    <sql>&#xd;&#xa; select B.metaSupervisor, B.meta as metaMes, B.metaDia metaDia, C.rvbase rvbase, D.valor as performance,A.productable_type productable_type, concat&#x28;year&#x28;coalesce&#x28;A.deal_at,A.created_at&#x29;&#x29;, right&#x28;concat&#x28;&#x27;00&#x27;,month&#x28;coalesce&#x28;A.deal_at,A.created_at&#x29;&#x29;&#x29;,2&#x29;,right&#x28;concat&#x28;&#x27;00&#x27;,day&#x28;coalesce&#x28;A.deal_at,A.created_at&#x29;&#x29;&#x29;,2&#x29;&#x29; tempo,coalesce&#x28;store,&#x27;Avante&#x27;&#x29; loja, coalesce&#x28;vendedor,0&#x29; vendedor, count&#x28;A.id&#x29; as quantidade, count&#x28;A.id&#x29; &#x2f; &#x28;B.meta&#x29; as ResultadoDiario  ,&#xd;&#xa;&#x28;select count&#x28;dia&#x29; from tempo where ano &#x3d; C.ano and mes &#x3d; C.mes&#xd;&#xa;and nome_dia_da_semana not in &#x28;&#x27;Sunday&#x27;,&#x27;Saturday&#x27;&#x29;&#xd;&#xa;&#x29; as diasUteis,&#xd;&#xa;&#x28;select count&#x28;dia&#x29; from tempo where ano &#x3d; C.ano and mes &#x3d; C.mes&#xd;&#xa;and nome_dia_da_semana not in &#x28;&#x27;Sunday&#x27;,&#x27;Saturday&#x27;&#x29;&#xd;&#xa;and dia &#x3c;&#x3d; 31&#xd;&#xa;&#x29; as diasUteisAteHoje,&#xd;&#xa;0 as diasUteisRestantes,&#xd;&#xa;&#xd;&#xa;count&#x28;A.id&#x29; &#x2f; &#xd;&#xa;&#x28;&#x28;select count&#x28;dia&#x29; from tempo where ano &#x3d; C.ano and mes &#x3d; C.mes&#xd;&#xa;and nome_dia_da_semana not in &#x28;&#x27;Sunday&#x27;,&#x27;Saturday&#x27;&#x29;&#xd;&#xa;and dia &#x3c;&#x3d; 31&#xd;&#xa;&#x29;&#x29;&#xd;&#xa; &#x2a; &#xd;&#xa;&#xd;&#xa;&#x28;select count&#x28;dia&#x29; from tempo where ano &#x3d; C.ano and mes &#x3d; C.mes&#xd;&#xa;and nome_dia_da_semana not in &#x28;&#x27;Sunday&#x27;,&#x27;Saturday&#x27;&#x29;&#xd;&#xa;&#x29; projecao&#xd;&#xa;, round&#x28;&#xd;&#xa;count&#x28;A.id&#x29; &#x2f; &#xd;&#xa;&#x28;&#x28;select count&#x28;dia&#x29; from tempo where ano &#x3d; C.ano and mes &#x3d; C.mes&#xd;&#xa;and nome_dia_da_semana not in &#x28;&#x27;Sunday&#x27;,&#x27;Saturday&#x27;&#x29;&#xd;&#xa;and dia &#x3c;&#